  "license": "ISC",
  "bugs": {
    "url": "https://github.com/finmily/nkoa/issues"
  },
  "homepage": "https://github.com/finmily/nkoa#readme",
  "dependencies": {
    "@types/koa": "^2.0.50",
    "kill-port": "^1.6.0",
    "koa": "^2.16.1",
    "koa-router": "^7.4.0"
  }
}
